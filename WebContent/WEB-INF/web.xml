<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID"
	version="3.0">
	<display-name>Eshine7</display-name>
	<welcome-file-list>
		<welcome-file>Index</welcome-file>
	</welcome-file-list>
	<context-param>
		<param-name>GroupAdmin</param-name>
		<param-value>*</param-value>
	</context-param>
	<context-param>
		<param-name>GroupUser</param-name>
		<param-value>Login, Logout, InsertArticle, UpdateArticle</param-value>
	</context-param>
	<context-param>
		<param-name>GroupGuest</param-name>
		<param-value>Login</param-value>
	</context-param>
	<context-param>
		<param-name>ContestManager</param-name>
		<param-value></param-value>
	</context-param>
	<context-param>
		<param-name>LoginURI</param-name>
		<param-value>Login</param-value>
	</context-param>
	<context-param>
		<param-name>PAGESIZE</param-name>
		<param-value>20</param-value>
	</context-param>
	<error-page>
		<exception-type>javax.servlet.ServletException</exception-type>
		<location>/errorException.jsp</location>
	</error-page>
	<error-page>
		<exception-type>java.lang.Throwable</exception-type>
		<location>/errorThrowable.jsp</location>
	</error-page>
	<error-page>
		<error-code>500</error-code>
		<location>/errorException.jsp</location>
	</error-page>
	<error-page>
		<error-code>405</error-code>
		<location>/errorException.jsp</location>
	</error-page>
	<error-page>
		<error-code>404</error-code>
		<location>/errorException.jsp</location>
	</error-page>
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
</web-app>